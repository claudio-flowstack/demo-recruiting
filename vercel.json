{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/demo-landing", "destination": "/demo-landing/index.html" },
    { "source": "/demo-landing-v2", "destination": "/demo-landing-v2/index.html" },
    { "source": "/demo-formular", "destination": "/demo-formular/index.html" },
    { "source": "/demo-danke", "destination": "/demo-danke/index.html" },
    { "source": "/((?!demo-).*)", "destination": "/index.html" }
  ]
}
